{
	"$schema": "https://json.schemastore.org/package",
	"author": "qwerty吃小庄",
	"dependencies": {
		"hexo": "^7.3.0",
		"hexo-filter-kroki": "^2.1.10",
		"hexo-generator-category": "^2.0.0",
		"hexo-generator-index": "^4.0.0",
		"hexo-generator-tag": "^2.0.0",
		"hexo-renderer-ejs": "^2.0.0",
		"hexo-renderer-pandoc": "^0.5.0",
		"hexo-renderer-stylus": "^3.0.1",
		"hexo-theme-a4": "^1.9.9"
	},
	"description": "qwerty吃小庄的个人博客",
	"devDependencies": {
		"prettier": "^3.6.2"
	},
	"funding": "https://ko-fi.com/qwertycxz",
	"hexo": {
		"version": "7.3.0"
	},
	"homepage": "TBD",
	"keywords": [
		"blog",
		"hexo"
	],
	"license": "(Apache-2.0 OR CC-BY-4.0)",
	"name": "qwertycxz.github.io",
	"private": true,
	"prettier": {
		"arrowParens": "avoid",
		"jsxSingleQuote": true,
		"proseWrap": "never",
		"quoteProps": "consistent",
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"repository": "https://github.com/qwertycxz/qwertycxz.github.io",
	"scripts": {
		"dev": "hexo --config hexo.json generate -w",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"generate": "hexo --config hexo.json generate",
		"page": "hexo --config hexo.json new page",
		"post": "hexo --config hexo.json new scaffold_post"
	}
}
